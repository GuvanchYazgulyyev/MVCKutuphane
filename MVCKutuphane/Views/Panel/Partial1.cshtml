@using MVCKutuphane.Models.Entity
@model List<TBLDuyuru>
@foreach (var x in Model)
{
    <div class="post">
        <div class="user-block">
            <img class="img-circle img-bordered-sm" src="~/AdminLTE-3.0.4/dist/img/avatar3.png" alt="user image">
            <span class="username">
                <a href="#">@x.DuyuruBaslik</a>
                @*<a href="#" class="float-right btn-tool"><i class="fas fa-times"></i></a>*@

            </span>
            <span class="description">@Convert.ToDateTime(x.Tatih).ToString("dd/MM/yyyy")</span>
        </div>
        <p>
            @x.Icerik
        </p>
    </div>

}