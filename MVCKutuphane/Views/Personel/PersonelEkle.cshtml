@model MVCKutuphane.Models.Entity.TBLPersonel
@{
    ViewBag.Title = "PersonelEkle";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h4>Personel Ekle Sayfası</h4>



<form method="post" class="form-group">
    <div>
        <label>Personel Ad Soyad:</label>
        @*<input class="form-control" type="text" name="Personel" />*@
        @Html.TextBoxFor(x => x.Personel, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.Personel, "", new { @style = "color:red" })
    </div>
    <div style="margin-top:15px">
        <button class="btn btn-success" id="Btn1">Personel Ekle</button>
    </div>
</form>
